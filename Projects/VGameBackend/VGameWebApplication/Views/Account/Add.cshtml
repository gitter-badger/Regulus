@model VGame.Project.FishHunter.Data.Account
@{
    ViewBag.Title = "Add";
}

<h2>Add</h2>


<div>

    @{
        using (Html.BeginForm("AddHandle", "Account", FormMethod.Post, new { enctype = "multipart/form-data", onsubmit = "return validateForm(this)" }))
        {
            <div class="form-group">
                @Html.Label("帳號")
                @Html.EditorFor( m => m.Name )
            </div>
            
            <div class="form-group">
                @Html.Label("密碼")
                @Html.EditorFor(m => m.Password)
            </div>

            <input type="submit" value="確認" class="btn btn-default" />
        }
    }
</div>

